<main>

    <!-- slider Area Start-->
    <div class="slider-area ">
        <!-- Mobile Menu -->
        <div class="slider-active">
            <div class="single-slider hero-overly  slider-height d-flex align-items-center"
                data-background="{{pages.homeHeader.bgImage}}">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-9 col-lg-9 col-md-9">
                            <div class="hero__caption">
                                <h1 style="font-size: 75px;"><span>{{pages.homeHeader.title}}</span> </h1>
                                <p>{{pages.homeHeader.descr}}</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- slider Area End-->

    <!-- image gallareya -->
    <div class="whole-wrap my-5 py-5">
        <div class="container my-5 py-5">
            <div class="my-5">
                <!-- <h3>Yoshlar texnoparkining qurilish loyihasi</h3> -->
                <div class="row mb-5">
                    <div class="col-lg-12">
                        <div class="section-tittle text-center">
                            <span>Surxondaryo</span>
                            <h1>Yoshlar texnoparkining qurilish loyihasi</h1>
                        </div>
                    </div>
                </div>
                <div class="row gallery-item">
                    {{#if resources}}
                        {{#each resources}}
                            <div class="col-md-4">
                                <a href="{{imageUrl}}" class="img-pop-up">
                                    <div class="single-gallery-image"
                                        style="background: url({{imageUrl}});">
                                    </div>
                                </a>
                            </div>
                        {{/each}}
                        {{else}}
                        <h2 class="text-center">Hozircha malumotlar yo'q</h2>
                    {{/if}}
                </div>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="mb-5 pb-5">
            <div class="col-lg-12">
                <div class="section-tittle text-center pb-4">
                    <h1>{{pages.maps.title}}</h1>
                </div>
            </div>
            {{{pages.maps.location}}}
        </div>
    </div>



